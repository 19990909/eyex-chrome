{
  "manifest_version": 2,
  "name": "EyeX Chrome App Example",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "1.0",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["scripts/background.js"]
    }
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  }
}
